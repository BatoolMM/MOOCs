{
  "source": {
    "include": ["../API/src", "../Frontend/src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "theme": "cerulean",
    "syntaxTheme": "dark"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["node_modules/docdash"],
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs",
    "theme_opts": {
      "default_theme": "white",
      "navType": "vertical",
      "collapse": true,
      "inverseNav": true
    },
    "readme": "README.md",
    "tutorials": "./docs/tutorials"
  }

}
