{
  "source": {
    "include": ["../backend_API/src", "../Frontend/src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/docdash/",
    "readme": "README.md"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "collapse": true,
    "search": true,
    "lineNumbers": true,
    "meta": {
      "title": "Backend API",
      "description": "Backend API",
      "url": "https://github.com/Realrichi3"
    }
  }
}
