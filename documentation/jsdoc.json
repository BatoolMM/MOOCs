{
  "source": {
    "include": ["../backend_API/src", "../Frontend/src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "theme": "yeti",
    "syntaxTheme": "dark"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/ink-docstrap/template",
    "readme": "README.md",
    "tutorials": "./docs/tutorials"
  }
}
